{
  "name": "nachos-ui",
  "description": "Nachos UI is a React Native component library",
  "version": "0.1.2",
  "humanName": "Nachos UI",
  "license": "MIT",
  "main": "./lib/index.js",
  "author": {
    "name": "Petr Brzek",
    "email": "petr@avocode.com"
  },
  "repository": "avocode/nachos-ui",
  "keywords": [
    "react",
    "react-component",
    "react-native",
    "ui-kit"
  ],
  "scripts": {
    "build": "babel src/ -d lib/",
    "build:web": "webpack --progress -p",
    "prepublish": "npm run build",
    "docs": "rsg \"docs/*.js\" -c \"styleguide.js\" -v",
    "docs:run": "http-server ./styleguide",
    "docs:prod": "npm run docs -- -r \"nachos-ui/docs\"",
    "start": "react-native run-ios",
    "start:web": "webpack-dev-server --port 3000 --config webpack.config.js --inline --hot --colors --quiet",
    "lint": "eslint src docs",
    "lint:fix": "eslint --fix src docs",
    "fmt": "prettier --print-width 70 --write '{src,docs}/**/*.js'; npm run lint:fix; gulp semistrip",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "preset": "react-native"
  },
  "dependencies": {
    "md5": "^2.2.1",
    "query-string": "^4.3.1",
    "react": "15.x.x",
    "react-native": ">=0.40.0",
    "react-native-vector-icons": "^4.0.0",
    "react-theme": "^0.1.4"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react-native": "^1.9.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "enzyme": "^2.7.0",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "eslint-plugin-standard": "^2.0.1",
    "estraverse-fb": "^1.3.1",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "gulp-replace": "^0.5.4",
    "http-server": "^0.9.0",
    "jest": "^18.1.0",
    "json-loader": "^0.5.4",
    "prettier": "^0.14.1",
    "raw-loader": "^0.5.1",
    "react": "15.x.x",
    "react-addons-test-utils": "15.x.x",
    "react-dom": "15.x.x",
    "react-native-mock": "^0.2.9",
    "react-native-web": "^0.0.68",
    "react-test-renderer": "^15.4.2",
    "rsg-alt": "^3.15.0",
    "string-replace-webpack-plugin": "^0.0.5",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  }
}
